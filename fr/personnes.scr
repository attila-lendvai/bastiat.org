;Alphabet à répéter sous chaque lettre
(define (alphabet)
(p :center *[
,(a :href :#a [A])
,(a :href :#b [B])
,(a :href :#c [C])
,(a :href :#d [D])
,(a :href :#e [E])
,(a :href :#f [F])
,(a :href :#g [G])
,(a :href :#h [H])
,(a :href :#i [I])
,(a :href :#j [J])
,(a :href :#k [K])
,(a :href :#l [L])
,(a :href :#m [M])
,(a :href :#n [N])
,(a :href :#o [O])
,(a :href :#p [P])
,(a :href :#q [Q])
,(a :href :#r [R])
,(a :href :#s [S])
,(a :href :#t [T])
,(a :href :#u [U])
,(a :href :#v [V])
,(a :href :#w [W])
,(a :href :#x [X])
,(a :href :#y [Y])
,(a :href :#z [Z])
]))

(defun
  lettre
  (x &rest r)
  (div :class "lettre" [
    ,(tag :a :name x)
    ,(subtitle (string-upcase x))
    ,(alphabet)
    ,(hrule)
  ] r (hrule))
)

(defun
	personne
	(id nom prenom dates &rest details)
	(div :class "personne" [
		,(tag :a :name id)
		,(p (span :class "nom" nom) " " prenom (span :class "dates" dates) )
	] details)
)

(defun
	personne1nom
	(id nom dates &rest details)
	(personne id nom "" dates details)
)

(defun
  bio
  (&rest x)
  (p :class "bio" x)
)

(defun
  references
  (&rest x)
  (div :class "references" [,(apply 'p* x)])
)
(defun
	person
	(id &rest details)
	(div :class "personne" :id id [
		,(tag :a :name id)
		,(p (span :class "nom" (nom (element id))) "  " (prenom (element id)) (span :class "dates" (dates (element id))) )
		,(p :class "bio" (biog (element id)))
	]
	(div :class "references" (apply 'p* details))
))



