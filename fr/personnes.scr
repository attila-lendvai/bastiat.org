(defun
  lettre
  (x &rest r)
  (div :class "lettre" [
    ,(tag :a :name x)
    ,(subtitle (string-upcase x))
  ] r )
)

(defun
  bio
  (&rest x)
  (p :class "bio" x)
)

(defun
  references
  (&rest x)
  (div :class "references" [,(apply 'p* x)])
)
(defun
	person
	(id &rest details)
	(div :class "personne" :id id [
		,(a :name id [ ])
		,(p (span :class "nom" (nom (element id))) " " (span :class "prenom" (prenom (element id))) (span :class "dates" (dates (element id))) )
		,(p :class "bio" (biog (element id)))
	]
	(div :class "references" (apply 'p* details))
))



