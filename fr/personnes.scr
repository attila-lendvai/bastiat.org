(defun
  lettre
  (x &rest r)
  (div :class "lettre" [
    ,(tag :a :name x)
    ,(subtitle x)
    ,(alphabet)
    ,(hrule)
  ] r (hrule))
)

(defun
	personne
	(id nom prenom dates &rest details)
	(div :class "personne" [
		,(tag :a :name id)
		,(p (span :class "nom" nom) " " prenom (span :class "dates" dates) )
	] details)
)

(defun
	personne1nom
	(id nom dates &rest details)
	(div :class "personne" [
		,(tag :a :name id)
		,(p (span :class "nom" nom) (span :class "dates" dates) )
	] details)
)



