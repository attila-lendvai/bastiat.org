(defun dn (x) (getf (rest x) :dn))
(defun dm (x) (getf (rest x) :dm))
;date visible (pour les cas compliqués):
(defun dv (x) (getf (rest x) :dv))
(defun dates (x) (if (dv x) (dv x) (strcat (write-to-string (dn x)) "–" (write-to-string (dm x)))))
(defun biog (x) (getf (rest x) :bio))



