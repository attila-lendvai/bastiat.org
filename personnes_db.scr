(defun filter-personnesdb (fun)
  (remove-if-not
   (lambda (x)
     (handler-case
   (apply fun x)
       (error (c)
   (error "Error while filtering bastiat-db entry: ~S.~%Entry: ~S~%"
    c x))))
   personnesdb))

(defmacro filtering-personnesdb (&body body)
  `(filter-personnesdb
    (lambda (id &key nom prenom dn dm pays categorie bio references &allow-other-keys) (declare (ignorable nom prenom dn dm pays categorie bio references id)) ,@body)))


